#include <stdio.h>
#include <math.h>
#include <time.h>
/*
Dentro da função main, colocar uma matriz 4x5, preencher essa matriz com uma função
de números aleatórios. Depois mostrar a função, identificar qual o seu maior elemento e a
posição através de uma função também.
*/
void PreencheFuncoes(int matriz[][5], int linhas, int colunas)
{
    srand(time(NULL));
    for (int i = 0; i < linhas; i++)
    {
        for (int j = 0; j < colunas; j++)
        {
            matriz[i][j] = rand() % 100;
        }
    }
}
void MostraFuncoes(int matriz[4][5], int linhas, int colunas)
{
    for (int i = 0; i < linhas; i++)
    {
        for (int j = 0; j < linhas; j++)
        {
            printf("%2d ", matriz[i][j]);
        }
        printf("\n");
    }
}
